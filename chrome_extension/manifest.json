{
  "manifest_version": 2,

  "name": "speakEZ",
  "description": "Enter in a speakEZ token and hear an audio recording.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "lgoo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.google.com/",
    "https://s-cord0.com/*",
    "activeTab",
    "storage"
  ]
}
